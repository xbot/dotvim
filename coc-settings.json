/* cSpell: disable */
{
    "coc.preferences.enableMessageDialog": true,
    "notification.statusLineProgress": false,
    "diagnostic.enable": true,
    "diagnostic.displayByAle": false,
    "diagnostic.floatConfig": {
        "border": true,
        "rounded": true,
        "title": "Diagnostic",
        "shadow": false,
        "close": false,
        "winblend": 20,
        "focusable": true
    },
    "hover.floatConfig": {
        "border": true,
        "rounded": true,
        "title": "",
        "shadow": false,
        "close": false,
        "winblend": 20,
        "focusable": false
    },
    "list.height": 20,
    "list.source.files.command": "rg",
    "list.source.files.args": ["--files","--no-ignore"],
	"list.insertMappings": {
		"<C-r>": "do:refresh",
		"<C-f>": "feedkeys:\\<C-f>",
		"<C-b>": "feedkeys:\\<C-b>",
		"<C-n>": "normal:j",
		"<C-p>": "normal:k",
		"<C-t>": "action:tabe",
		"<C-x>": "call:MyFunc",
		"<C-v>": "eval:@@"
	},
